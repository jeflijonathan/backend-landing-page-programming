{
  "name": "expressjs-restful-api-template",
  "version": "0.0.1",
  "author": "Fanesz",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "seed": "node src/seeders/index.js",
    "seed:role": "node src/seeders/role.seeder.js",
    "seed:user": "node src/seeders/user.seeder.js",
    "migrate:base": "bunx db-migrate --config ./migrations/migration.config.js -e default",
    "migrate:schema": "bun run migrate:base -- -m ./migrations/schema",
    "migrate:schema:create": "bun run migrate:schema create",
    "migrate:schema:up": "bun run migrate:schema up",
    "migrate:schema:down": "bun run migrate:schema down"
  },
  "dependencies": {
    "bcrypt": "^6.0.0",
    "bun": "^1.3.8",
    "cors": "^2.8.6",
    "db-migrate": "^0.11.14",
    "db-migrate-mysql": "^3.0.0",
    "dotenv": "^17.2.2",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "jsonwebtoken": "^9.0.3",
    "moment": "^2.30.1",
    "multer": "^2.0.2",
    "pdf-lib": "^1.17.1",
    "pg": "^8.17.2",
    "pg-hstore": "^2.3.4",
    "redis": "^5.10.0",
    "sequelize": "^6.37.7",
    "sharp": "^0.34.5",
    "tinify": "^1.8.2",
    "uuid": "^13.0.0",
    "yup": "^1.7.1"
  },
  "devDependencies": {
    "eslint": "^9.36.0",
    "globals": "^16.4.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2"
  }
}